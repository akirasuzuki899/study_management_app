<%= form_with(model: @study_note, local: true) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :name %>
  <%= f.collection_select :study_material_id, current_user.study_materials.all, :id, :name, include_blank: "選択して下さい" %>

  <%= f.label :page_number %>
  <%= f.number_field :page_number %>

  <%= f.label :content %>
  <%= f.rich_text_area :content %>

  
  <%= f.submit "Post" %>
<% end %>